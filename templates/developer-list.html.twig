{% extends "base.html.twig" %}
{% block header %}
    {{ parent() }}
{% endblock %}

{% block body %}
    {{ parent() }}
{% endblock %}
{% block context %}
    <table class="table">
        <thead>
        <tr>
            <th scope="col">name</th>
            <th scope="col">duration</th>
            <th scope="col">difficulty</th>
        </tr>
        </thead>
        <tbody>
        {% for value in data  %}
        <tr>
            <td>{{ value.name }}</td>
            <td>{{ value.duration }}</td>
            <td>{{ value.difficulty }}</td>
        </tr>
    {% endfor %}
        </tbody>
    </table>
{% endblock %}
